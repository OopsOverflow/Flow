{
  "name": "flow",
  "description": "A Marcelle Application with SvelteKit",
  "version": "0.0.0",
  "keywords": [
    "marcelle"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint . --ext .js,.ts,.svelte --fix",
    "format": "prettier --plugin-search-dir . --write --ext .js,.ts,.svelte ."
  },
  "dependencies": {
    "@marcellejs/core": "^0.6.0",
    "spotify-web-api-node": "^5.0.2",
    "chart.js": "^4.2.1",
    "svelte-chartjs": "^3.1.2",
    "svelte-mp3": "^1.2.0"
  },
  "devDependencies": {
    "@marcellejs/design-system": "^0.6.0",
    "@marcellejs/devtools": "^0.6.0",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/adapter-vercel": "^2.0.2",
    "@sveltejs/kit": "^1.5.0",
    "@types/spotify-web-api-node": "^5.0.7",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^4.1.1"
  }
}
